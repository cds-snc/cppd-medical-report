{
  "name": "cppd-medical-report",
  "version": "0.0.1",
  "description": "",
  "author": "CDS",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "12.13.1"
  },
  "scripts": {
    "build": "webpack --mode=development",
    "build:dev": "webpack --mode=development",
    "dev": "cross-env nodemon --exec npm run dev:dist",
    "dev:dist": "webpack --mode=development && cross-env NODE_ENV=development  node ./bin/www",
    "start": "cross-env NODE_ENV=production node ./bin/www",
    "lint": "node node_modules/eslint/bin/eslint.js config utils routes bin/www app.js",
    "prettier": "prettier --write '**/*.{ts,js,css,html}'",
    "db:install": "docker pull postgres:latest",
    "db:start": "docker run --rm -p 5432:5432 -d --name postgres-dev postgres:latest",
    "db:stop": "docker stop postgres-dev",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all",
    "test": "node node_modules/jest/bin/jest.js --coverage",
    "test:watch": "node node_modules/jest/bin/jest.js --watch",
    "postinstall": "patch-package"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm test"
    }
  },
  "dependencies": {
    "@cdssnc/webpack-starter": "^2.0.2",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^1.3.3",
    "copy-template-dir": "^1.4.0",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.10",
    "csurf": "^1.10.0",
    "dotenv": "^8.2.0",
    "dynamoose": "^1.11.1",
    "express": "~4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.2.0",
    "helmet": "^3.21.2",
    "i18n": "^0.8.4",
    "knex": "^0.20.1",
    "memorystore": "^1.6.1",
    "mini-css-extract-plugin": "^0.8.0",
    "nunjucks": "^3.2.0",
    "pg": "^7.14.0",
    "pg-hstore": "^2.3.3",
    "request": "^2.88.0",
    "sequelize": "^5.21.2"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "^4.0.0",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "acorn": "^7.1.0",
    "cheerio": "^1.0.0-rc.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "fse": "^4.0.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "npm": "^6.13.1",
    "nodemon": "^2.0.1",
    "patch-package": "^6.2.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "supertest-session": "^4.0.0",
    "tailwindcss": "^1.1.3",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.2",
    "vue-scrollto": "^2.17.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack-cli": "^3.3.10"
  },
  "nodemonConfig": {
    "ext": "js,json,njk,scss,vue",
    "ignore": [
      "public/dist/**/*.*",
      "sessions/**/*.*"
    ]
  }
}
